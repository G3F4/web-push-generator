{
  "name": "web-push-generator-server",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r dotenv/config $npm_package_main",
    "dev": "ts-node-dev --respawn --transpileOnly -r dotenv/config $npm_package_main",
    "build": "rm -rf build && echo \"building server\" && mkdir build && cp src/index.ts build",
    "test": "echo \"no test specified\" && exit 0",
    "lint": "eslint -r dotenv/config './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint -r dotenv/config './src/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@types/node": "12.12.6",
    "@types/web-push": "3.3.0",
    "dotenv": "8.2.0",
    "fastify": "2.10.0",
    "ts-node": "8.4.1",
    "typescript": "3.7.2",
    "web-push": "3.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "prettier": "1.18.2",
    "ts-node-dev": "1.0.0-pre.44"
  }
}
